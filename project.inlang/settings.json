{
	"sourceLanguageTag": "en-US",
	"languageTags": ["en-US", "pt-BR", "it-IT", "zh-CN"],
	"modules": ["https://cdn.jsdelivr.net/npm/@inlang/plugin-json@latest/dist/index.js"],
	"plugin.inlang.json": {
		"pathPattern": "./locales/{languageTag}.json"
	},
	"messageLintRuleLevels": {
		"messageLintRule.inlang.emptyPattern": "error",
		"messageLintRule.inlang.identicalPattern": "warning",
		"messageLintRule.inlang.camelCaseId": "warning",
		"messageLintRule.inlang.missingTranslation": "warning",
		"messageLintRule.inlang.snakeCaseId": "warning",
		"messageLintRule.inlang.validJsIdentifier": "warning",
	}
}
